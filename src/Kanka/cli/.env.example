# Kanka API Configuration
# Copy this file to .env in the project root directory and fill in your values

# Your Kanka API token (required)
# Obtain this from your Kanka account settings
KANKA_API_KEY=your_api_key_here

# API Base URL (optional, defaults to https://app.kanka.io/api/1.0)
KANKA_BASE_URL=https://app.kanka.io/api/1.0
